use ieee.numeric_std.all;&#13;
use std.textio.all;&#13;
&#13;
entity dmem is&#13;
	port(&#13;
		clk : in  std_logic;&#13;
		we  : in  std_logic;&#13;
		a   : in  std_logic_vector(31 downto 0);&#13;
		wd  : in  std_logic_vector(31 downto 0);&#13;
		rd  : out std_logic_vector(31 downto 0)&#13;
	);&#13;
end dmem;&#13;
&#13;
architecture RTL of dmem is&#13;
	type memoryArray is array (63 downto 0) of std_logic_vector(31 downto 0);&#13;
	impure function init_ram return memoryArray is&#13;
		file outputText     : text;&#13;
		variable outputLine : line;&#13;
		variable temporal  : std_logic_vector(31 downto 0);&#13;
		variable ram       : memoryArray := (others =&gt; (others =&gt; '0'));&#13;
	begin&#13;
		file_open(outputText, "C:/Users/Tasos/Desktop/diplomatiki/data_memfile.dat", write_mode);&#13;
		for index in 0 to 127 loop&#13;
			if (endfile(outputText)) then&#13;
				exit;&#13;
			else&#13;
				readline(outputText, outputLine);&#13;
				write(outputLine, temporal);&#13;
				ram(index) := temporal;&#13;
			end if;&#13;
		end loop;&#13;
		file_close(outputText);&#13;
		return ram;&#13;
	end init_ram;&#13;
	signal ram : memoryArray := init_ram;&#13;
begin&#13;
	proc : process(clk)&#13;
	begin&#13;
		if (rising_edge(clk)) then&#13;
			if (we = '1') then&#13;
				ram(to_integer(unsigned(a(7 downto 2)))) &lt;= wd;&#13;
			end if;&#13;
		end if;&#13;
	end process proc;&#13;
	rd &lt;= ram(to_integer(unsigned(a(7 downto 2)))) after 50 ns;&#13;
end RTL;&#13;
</a>
